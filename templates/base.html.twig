<!DOCTYPE html>
<html lang="fr">
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>{% block title %}{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="{{ asset('assets/stylesheets/reset.css') }}">
            <link rel="stylesheet" href="{{ asset('assets/stylesheets/home.css') }}">
        {% endblock %}
    </head>
    <body>
        {% include 'components/_nav.html.twig' %}
        <main>
            {% block content %}{% endblock %}
        </main>
        {% include 'components/_footer.html.twig' %}
        {% block javascripts %}
            <script src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
            <script src="{{ asset('assets/javascripts/card.js') }}"></script>
        {% endblock %}
    </body>
</html>